[Unit]
Description=set multiple CFS tunings in sched debugfs
After=sysinit.target

[Service]
Type=oneshot
RemainAfterExit=true
ExecStart=/usr/lib/steamos/steamos-cfs-debugfs-settings

[Install]
WantedBy=multi-user.target
